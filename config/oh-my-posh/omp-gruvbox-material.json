{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "os": "#7c6f64",
        "closer": "p:grey2",
        "pink": "#d3869b",
        "purple": "#945e80",
        "lavender": "#B7BDF8",
        "blue": "#80aa9e",
        "blue_strong": "#0f3a42",
        "aqua": "#8bba7f",
        "yellow": "#e9b143",
        "orange": "#f28534",
        "fg1": "#e2cca9",
        "bg5": "#665c54",
        "grey2": "#a89984"
    },
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "p:blue",
                    "properties": {
                        "style": "full"
                    },
                    "style": "plain",
                    "template": "{{ .Path }} ",
                    "type": "path"
                },
                {
                    "foreground": "p:aqua",
                    "foreground_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#e9b143{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#e2cca9{{ end }}",
                        "{{ if gt .Ahead 0 }}#945e80{{ end }}",
                        "{{ if gt .Behind 0 }}#945e80{{ end }}"
                    ],
                    "properties": {
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "native_fallback": true
                    },
                    "template": "<#a89984>on</> {{ .HEAD }}{{ if gt .Ahead 0 }} \u2191{{ .Ahead }}{{ end }}{{ if gt .Behind 0 }} \u2193{{ .Behind }}{{ end }}{{ if .Staging.Changed }}<#f28534> \uf046 {{ .Staging.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}",
                    "style": "accordion",
                    "type": "git"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "p:closer",
                    "style": "plain",
                    "template": "at",
                    "type": "text"
                },
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "p:pink",
                    "properties": {
                        "time_format": "15:04:05"
                    }
                },
                {
                    "foreground": "p:closer",
                    "style": "plain",
                    "template": "\uf105",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 3
}
